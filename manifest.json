{
  "name": "Tabdown",
  "description": "Save all your open tabs as links into a markdown or plaintext file.",
  "version": "0.0.1",
  "manifest_version": 3,
  "icons": {
    "128": "icon-128.png"
  },
  "action": {
    "default_popup": "index.html",
    "default_icon": "icon-128.png"
  },
  "permissions": [
    "tabs"
  ]
}
